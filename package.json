{
  "name": "cypress-ci-api-test",
  "version": "1.0.0",
  "description": "Cypress E2E runner can also test Rest and other APIs",
  "main": "index.js",
  "private": true,
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:record": "cypress run --record --key af8017de-d651-4f29-b510-944e0c1cbcd1",
    "start": "json-server db.json",
    "test": "run-p --race start cy:run",
    "test:ci": "run-p --race start cy:run:record"
  },
  "keywords": [],
  "author": "sj",
  "license": "ISC",
  "devDependencies": {
    "cypress": "4.4.1",
    "npm-run-all": "4.1.5"
  },
  "dependencies": {
    "json-server": "0.15.1"
  }
}
